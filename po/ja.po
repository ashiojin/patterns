#~ "        }\n"
#~ "    }\n"
#~ "\n"
#~ "    pub struct Bootp(); // no additional metadata\n"
#~ "\n"
#~ "    impl ProtoKind for Bootp {\n"
#~ "        type AuthInfo = bootp::AuthInfo;\n"
