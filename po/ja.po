#~ "        }\n"
#~ "    }\n"
#~ "}\n"
#~ "\n"
#~ "use proto_trait::ProtoKind; // keep internal to prevent impls\n"
#~ "pub use proto_trait::{Nfs, Bootp}; // re-export so callers can see them\n"
#~ "\n"
#~ "struct FileDownloadRequest<P: ProtoKind> {\n"
